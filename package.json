{
    "name": "@keycloakify/login-ui-storybook",
    "version": "250004.2.0",
    "description": "Storybook stories for @keycloakify/login-ui",
    "repository": {
        "type": "git",
        "url": "git://github.com/keycloakify/login-ui-storybook"
    },
    "scripts": {
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej",
    "license": "MIT",
    "keywords": [],
    "homepage": "https://keycloakify.dev",
    "devDependencies": {
        "@types/node": "^22.15.18",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "prettier": "^3.3.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
